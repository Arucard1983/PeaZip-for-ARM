   fun_status:=fun;
   s:=Form_peach.EditOpenIn.Text;
   if s<>'' then if s[length(s)]=directoryseparator then setlength(s,length(s)-1);
   s:=uextractfilepath(s);
   if checkUNCpath(s) then
      begin
      s1:=copy(s,1,length(s)-1);
