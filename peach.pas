for i:=1 to 8 do csrec[i]:='';
j:=1;
for i:=1 to 8 do
   if ufileexists(srec[i]) then
      begin
      csrec[j]:=srec[i];
      j:=j+1;
      end;
Form_peach.mRecent1.Caption:=csrec[1];
