function modcolor(col:TColor; dr,dg,db:Single):TColor;
var
  r, g, b: Byte;
begin
  {$IFDEF MSWINDOWS}
  col := ColorToRGB(col);
  r := GetRValue(col);
  g := GetGValue(col);
  b := GetBValue(col);
  r := trunc( r * dr);
  g := trunc( g * dg);
  b := trunc( b * db);
  result := RGB(r, g, b);
  {$ELSE}
  result:=clBtnShadow;
  {$ENDIF}
end;

